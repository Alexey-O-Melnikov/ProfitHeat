@model ProfitHeat.Domain.Radiator

<label for="radiator">Радиатор</label>
<ul>
    <li>
        @{
            Html.RenderAction("_MaterialsRadiators", new { materialsRadiatorID = Model.MaterialRadiatorID });
        }
    </li>
    <li>
        @{
            Html.RenderAction("_ManufacturerRadiators", new { manufacturerRadiatorID = Model.ManufacturerRadiatorID });
        }
    </li>
    <li>
        @{
            Html.RenderAction("_ModelRadiator", new
            {
                radiatorID = Model.RadiatorID,
                materialRadiatorID = ((ProfitHeat.Domain.MaterialRadiator)TempData.Peek("materialsRadiator")).MaterialRadiatorID,
                manufacturerRadiatorID = ((ProfitHeat.Domain.ManufacturerRadiator)TempData.Peek("manufacturerRadiator")).ManufacturerRadiatorID
            });
        }
    </li>
</ul>