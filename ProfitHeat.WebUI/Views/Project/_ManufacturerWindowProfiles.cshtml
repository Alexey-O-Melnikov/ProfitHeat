@model List<ProfitHeat.Domain.ManufacturerWindowProfile>

<div class="@*form-group*@">
    <label for="ManufacturerWindowProfiles">Производитель профиля</label>
    <div class="" >
        <input onchange="changeManufacturerWindowProfiles()" id="manufacturerWindowProfil" type="text" list="ManufacturerWindowProfiles" class="form-control" value="@ViewBag.ManufacturerWindowProfile.TitleCompany" data-url="@Url.Action("_WindowProfiles", "Project")"/>
        <datalist id="ManufacturerWindowProfiles">
            @foreach (var item in Model)
            {
                <option>@item.TitleCompany</option>
            }
        </datalist>
    </div>
</div>